{
    "name": "aras/vinted_shipment_discount",
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Aras\\ShipmentDiscount\\": "src/",
            "Aras\\ShipmentDiscountTests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Aras"
        }
    ],
    "scripts": {
        "start": "php public/index.php",
        "test": "./vendor/bin/phpunit tests"
    },
    "config": {
        "process-timeout": 300,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {},
        "use-include-path": false
    }
}
